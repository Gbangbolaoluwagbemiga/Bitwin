{
  "name": "Bitwin-tests",
  "version": "1.0.0",
  "description": "Run unit tests on this project.",
  "type": "module",
  "private": true,
  "scripts": {
    "test": "vitest run",
    "test:report": "vitest run -- --coverage --costs",
    "test:watch": "chokidar \"tests/**/*.ts\" \"contracts/**/*.clar\" -c \"npm run test:report\"",
    "deploy:batch": "node scripts/deploy_batch_simple.js",
    "deploy:100": "CONTRACT_COUNT=100 TOTAL_FEE_BUDGET_USTX=500000 DEPLOY_INTERVAL_MINUTES=2 CONTRACT_TEMPLATE=minimal VERIFY_TIMEOUT_MINUTES=30 node scripts/deploy_batch_simple.js"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@stacks/clarinet-sdk": "^3.9.0",
    "@stacks/network": "^7.3.1",
    "@stacks/transactions": "^7.2.0",
    "@stacks/wallet-sdk": "^7.2.0",
    "node-fetch": "^3.3.2",
    "@types/node": "^24.4.0",
    "chokidar-cli": "^3.0.0",
    "dotenv": "^17.2.3",
    "vitest": "^4.0.7",
    "vitest-environment-clarinet": "^3.0.0"
  }
}
